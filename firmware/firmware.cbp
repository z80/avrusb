<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="firmware" />
		<Option pch_mode="2" />
		<Option compiler="avrgcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin\Debug\firmware.elf" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="avrgcc" />
				<Compiler>
					<Add option="-g" />
					<Add directory="src" />
					<Add directory="usbdrv" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin\Release\firmware.elf" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj\Release\" />
				<Option type="1" />
				<Option compiler="avrgcc" />
				<Compiler>
					<Add option="-Os" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-mmcu=atmega8" />
			<Add option="-Wall" />
			<Add option="-DF_CPU=12000000UL" />
		</Compiler>
		<Linker>
			<Add option="-mmcu=atmega8" />
			<Add option="-Wl,-Map=$(TARGET_OUTPUT_FILE).map,--cref" />
		</Linker>
		<ExtraCommands>
			<Add after="avr-size --mcu=atmega8 --format=avr $(TARGET_OUTPUT_FILE)" />
			<Add after="avr-objcopy -O ihex -R .eeprom -R .eesafe $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).hex" />
			<Add after="avr-objcopy --no-change-warnings -j .eeprom --change-section-lma .eeprom=0 -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).eep.hex" />
			<Add after="avr-objcopy -O binary -R .eeprom -R .eesafe $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).bin" />
			<Add after="avr-objcopy --no-change-warnings -j .eeprom --change-section-lma .eeprom=0 -O binary $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).eep.bin" />
			<Add after='cmd /c &quot;avr-objdump -h -S $(TARGET_OUTPUT_FILE) &gt; $(TARGET_OUTPUT_FILE).lss&quot;' />
		</ExtraCommands>
		<Unit filename="src\main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src\usbconfig.h" />
		<Unit filename="usbdrv\asmcommon.inc" />
		<Unit filename="usbdrv\oddebug.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="usbdrv\oddebug.h" />
		<Unit filename="usbdrv\usbdrv.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="usbdrv\usbdrv.h" />
		<Unit filename="usbdrv\usbdrvasm.S" />
		<Unit filename="usbdrv\usbdrvasm.asm" />
		<Unit filename="usbdrv\usbdrvasm12.inc" />
		<Unit filename="usbdrv\usbdrvasm128.inc" />
		<Unit filename="usbdrv\usbdrvasm15.inc" />
		<Unit filename="usbdrv\usbdrvasm16.inc" />
		<Unit filename="usbdrv\usbdrvasm165.inc" />
		<Unit filename="usbdrv\usbdrvasm18-crc.inc" />
		<Unit filename="usbdrv\usbdrvasm20.inc" />
		<Unit filename="usbdrv\usbportability.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
